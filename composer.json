{
  "name": "rossel/mwu-sdk",
  "description": "PHP library for connecting with MWU pick to light system",
  "type": "library",
  "license": "",
  "keywords": [
    "MWU",
    "pick to light",
    "PTL",
    "MWU series",
    "connector",
    "sdk"
  ],
  "authors": [
    {
      "name": "Florian Vaneste"
    }
  ],
  "autoload": {
    "psr-4": {
      "MwuSdk\\": "src/",
      "MwuSdkTest\\": "tests/"
    },
    "autoload-dev": {
      "psr-4": {
        "Doctrine\\Bundle\\DoctrineBundle\\Tests\\": "tests/"
      }
    }
  },
  "require": {
    "php": "^8.2",
    "symfony/validator": "^7.1",
    "symfony/yaml": "^7.1",
    "symfony/options-resolver": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^11",
    "phpstan/phpstan": "^1.11"
  }
}
